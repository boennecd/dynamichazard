n_fails <- by(
  tmp_dat$X, tmp_dat$X$model, function(X)
    tapply(X$Y, X$t, sum))

fail_ratios <- mapply("/", n_fails, n_obs)

plot(c(1, 60), c(0, max(unlist(smart_12_mean))), type = "n",
     xlab = "Month", ylab = "Number of power cycles")

for(i in seq_along(smart_12_mean)){
